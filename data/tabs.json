{
  "notifications": {
    "filter": {
      "user_state": {
        "$lt": 0
      }
    },
    "sort_key": "release_date",
    "hide_unreleased": true
  },
  "movie": {
    "filter": {
      "type": 1
    },
    "reverse": true
  },
  "tv": {
    "filter": {
      "type": 0
    },
    "reverse": true
  },
  "favourites": {
    "filter": {
      "user_state": 2
    },
    "reverse": true
  },
  "favourites-s": {
    "filter": {
      "user_state": 2,
      "ratings.user.vote_count": {
        "$gt": 0
      }
    },
    "sort_key": "ratings.user.vote_average",
    "reverse": true
  },
  "watched": {
    "filter": {
      "user_state": {
        "$gt": 0
      }
    }
  },
  "continue-watching": {
    "filter": {
      "user_state": 0
    },
    "sort_key": false
  },
  "anime": {
    "filter": {
      "genre_ids": 7424
    },
    "reverse": true
  },
  "anime-watched": {
    "filter": {
      "genre_ids": 7424,
      "user_state": {
        "$gt": 0
      }
    },
    "reverse": true
  },
  "anime-list": {
    "filter": {
      "genre_ids": 7424,
      "user_state": {
        "$lt": 0
      }
    },
    "reverse": true
  },
  "animation": {
    "filter": {
      "genre_ids": {
        "$eq": 16,
        "$ne": 7424
      }
    }
  },
  "kids": {
    "filter": {
      "$or": [
        {
          "genre_ids": {
            "$in": [10751, 10762]
          }
        },
        {
          "rated": {
            "$in": ["0"]
          }
        }
      ]
    }
  },
  "all-time-popular-tmdb": {
    "filter": {
      "ratings.tmdb.vote_count": {
        "$gt": 0
      }
    },
    "sort_key": "ratings.tmdb.vote_count",
    "reverse": true
  },
  "all-time-popular": {
    "filter": {
      "ratings.imdb.vote_count": {
        "$gt": 0
      }
    },
    "sort_key": "ratings.imdb.vote_count",
    "reverse": true
  },
  "popular": {
    "filter": {
      "popularity": {
        "$ne": null
      }
    },
    "sort_key": "popularity",
    "reverse": true
  },
  "my list": {
    "filter": {
      "user_state": -1
    },
    "sort_key": false,
    "hide_unreleased": true
  },
  "new": {
    "sort_key": false,
    "reverse": true
  },
  "watch-it-again": {
    "filter": {
      "user_state": {
        "$gt": 0
      }
    },
    "sort_key": false,
    "hide_unreleased": true
  },
  "recently": {
    "sort_key": "release_date",
    "hide_unreleased": true,
    "reverse": true
  },
  "soon": {
    "sort_key": "release_date",
    "only_unreleased": true,
    "reverse": true
  },
  "top-rated-tmdb": {
    "filter": {
      "ratings.tmdb.vote_count": {
        "$gte": 200
      }
    },
    "sort_key": "ratings.tmdb.vote_average",
    "hide_unreleased": true,
    "reverse": true
  },
  "top-rated": {
    "filter": {
      "user_rating": {
        "$gte": 0
      }
    },
    "sort_key": "user_rating",
    "hide_unreleased": true
  },
  "rated": {
    "filter": {
      "user_rating": {
        "$gte": 0
      }
    },
    "reverse": true
  },
  "top-rated-imdb": {
    "filter": {
      "ratings.imdb.vote_count": {
        "$gte": 200
      }
    },
    "sort_key": "ratings.imdb.vote_average",
    "hide_unreleased": true,
    "reverse": true
  },
  "top-rated-rt": {
    "filter": {
      "ratings.rt.vote_average": {
        "$gte": 0
      }
    },
    "sort_key": "ratings.rt.vote_average",
    "hide_unreleased": true,
    "reverse": true
  },
  "unfiltered": {
    "sort_key": false
  },
  "temp": {
    "filter": {
      "type": 1,
      "user_state": {
        "$lt": 0
      },
      "ratings.imdb.vote_average": {
        "$gte": 7
      }
    },
    "hide_unreleased": true,
    "sort_key": "ratings.imdb.vote_count"
  },
  "update": {
    "sort_key": "updated_at"
  },
  "index": {
    "sort_key": "user_index",
    "reverse": false
  },
  "index-r": {
    "sort_key": "user_index",
    "reverse": true
  },
  "ex": {
    "filter": {
      "user_state": {
        "$eq": null
      }
    },
    "reverse": true
  },
  "company": {
    "sort_key": "popularity"
  },
  "key": {
    "reverse": true
  }
}
